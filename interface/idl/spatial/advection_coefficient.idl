/**
 * # AdvectionCoefficient class
 */
[Prefix="libsbml::"]
interface AdvectionCoefficient {
  /**
   * Returns the value of the "variable" attribute of this
   * AdvectionCoefficient.
   *
   * @return {string} the value of the "variable" attribute of this AdvectionCoefficient
   * as a string.
   */
  DOMString getVariable();


  /**
   * Returns the value of the "coordinate" attribute of this
   * AdvectionCoefficient.
   *
   * @return {CoordinateKind_t} the value of the "coordinate" attribute of this
   * AdvectionCoefficient as a CoordinateKind_t.
   */
  libsbml__idl__CoordinateKind_t getCoordinate();


  /**
