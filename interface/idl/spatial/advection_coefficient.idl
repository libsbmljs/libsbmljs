  long setVariable(DOMString variable);


  /**
   * Sets the value of the "coordinate" attribute of this AdvectionCoefficient.
   *
   * @param {CoordinateKind_t} coordinate value of the "coordinate" attribute to
   * be set.
   */
  long setCoordinate(libsbml__idl__CoordinateKind_t coordinate);


  /**
   * Unsets the value of the "variable" attribute of this AdvectionCoefficient.
   */
  long unsetVariable();


  /**
   * Unsets the value of the "coordinate" attribute of this
   * AdvectionCoefficient.
   */
  long unsetCoordinate();
};

AdvectionCoefficient implements SBase;